{
  "name": "projectname",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "generate-apk": "cd android && ./gradlew app:assembleRelease",
    "debug-apk": "cd android && ./gradlew app:assembleDebug && open android/app/build/outputs/apk/debug",
    "temp-release": "yarn generate-apk && yarn open-explorer",
    "open-explorer": "open android/app/build/outputs/apk/release",
    "pod": "cd ios && pod install && cd .. && yarn ios",
    "android-release": "react-native run-android --variant=release",
    "clean": "cd android && ./gradlew clean && cd ..",
    "bulldozer": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "set:env:debug": "node ./src/Scripts/env.js --env debug",
    "set:env:staging": "node ./src/Scripts/env.js --env staging",
    "set:env:release": "node ./src/Scripts/env.js --env release",
    "run:android:debug": "yarn set:env:debug && yarn android --variant debug",
    "run:android:staging": "yarn set:env:staging && yarn android --variant staging",
    "run:android:release": "yarn set:env:release && yarn android --variant release",
    "run:ios:debug": "yarn set:env:debug && yarn ios --configuration Debug",
    "run:ios:staging": "yarn set:env:staging && yarn ios --configuration Staging",
    "run:ios:release": "yarn set:env:release && yarn ios --configuration Release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/material-top-tabs": "^6.2.4",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.1",
    "axios": "^1.1.3",
    "react": "17.0.2",
    "react-native": "0.66.1",
    "react-native-bootsplash": "^4.1.4",
    "react-native-dropdown-picker": "^5.4.3",
    "react-native-dynamic-vector-icons": "^1.2.0",
    "react-native-event-listeners": "^1.0.7",
    "react-native-gesture-handler": "^2.7.0",
    "react-native-mmkv": "^2.4.3",
    "react-native-pager-view": "^6.0.1",
    "react-native-reanimated": "^2.3.0-beta.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-tab-view": "^3.2.1",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "rn-animated-snackbar": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.5",
    "@types/react-native": "^0.67.4",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "@types/react": "^18.0.5",
    "@types/react-native": "^0.67.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
